<div class="outer-container post-cont">
	<div class="row">
		<div class="col-xs-12 proj-cont">
			<p class="container back-btn"><a href="/work">&larr;</a></p>
			<article>
				<div class="post">
					<header class="container post-words">
						<div class="col-xs-12 col-sm-3 title">
							<h5 class="p-sub-head">{{data.project.title}}</h5>
							<div class="categories">
								<h5>{{{categoryList data.project.categories}}}</h5>
							</div>
							<div class="post-categories">
								<h5>Share this project</h5>
								<ul class="social-list">	
									<a href="http://www.facebook.com/sharer.php?u=https://sidechain.studio" target="_blank">			
							    	<li class="icon">
							    		<i class="fa fa-facebook" aria-hidden="true"></i>	
							       </li>		
							     </a>						
									<a href="https://twitter.com/share?url=https://sidechain.studio&amp;text=Sidechain%20are%20my%20friends&amp;hashtags=sidechain" target="_blank">
										<li class="icon">
											<i class="fa fa-twitter" aria-hidden="true"></i>
										</li>	
									</a>							
									<a href="https://plus.google.com/share?url=https://sidechain.studio" target="_blank">
										<li class="icon">
											<i class="fa fa-google-plus" aria-hidden="true"></i>
										</li>
									</a>																
									<a href="mailto:?Subject=Sidechain&amp;Body=I%20saw%20this%20and%20thought%20you%20might%20like%20a%20sidechain %20 https://sidechain.studio">
										<li class="icon">
											<i class="fa fa-envelope-o" aria-hidden="true"></i>
										</li>
									</a>									
								</ul>
							</div>
						</div>
						<div class="col-xs-12 col-sm-offset-1 col-sm-8 brief">
							{{{data.project.content.brief}}}
						</div>
						<div class="col-xs-12 col-sm-2 post-categories">
							<!--<div class="categories">
								<h5>{{{categoryList data.project.categories}}}</h5>
							</div>-->
						</div>
					</header>
					{{{data.project.videoUrl}}}
					{{{data.project.content.extended}}}
					<div class="container project-footer">
						<div class="col-xs-12 col-sm-3 post-categories">
							<div class="categories col-xs-12">
								<h5>{{{categoryList data.project.categories}}}</h5>
							</div>
							<div class="col-xs-12 proj-footer-share">
								<h5>Share this project</h5>
								<ul class="social-list">								
							    	<li class="icon">
							    		<a href="http://www.facebook.com/sharer.php?u=https://sidechain.studio" target="_blank">
							    			<i class="fa fa-facebook" aria-hidden="true"></i>
							    		</a>
							        </li>								
									<li class="icon">
										<a href="https://twitter.com/share?url=https://sidechain.studio&amp;text=Sidechain%20are%20my%20friends&amp;hashtags=sidechain" target="_blank">
											<i class="fa fa-twitter" aria-hidden="true"></i>
										</a>
									</li>								
